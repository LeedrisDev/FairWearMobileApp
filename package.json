{
  "name": "fairwear-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config=config/vite.config.ts",
    "build": "tsc --project config/tsconfig.json && vite --config=config/vite.config.ts build",
    "preview": "vite --config=config/vite.config.ts preview",
    "install-legacy": "npm install --legacy-peer-deps",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src __tests__",
    "test": "jest --config config/jest.config.ts",
    "test:coverage": "jest --config config/jest.config.ts --coverage=true"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cliui": "^8.0.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.1",
    "jest-transform-stub": "^2.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "^0.14.4",
    "workbox-window": "^6.5.4"
  },
  "eslintConfig": {
    "extends": [
      "./config/.eslintrc.json"
    ]
